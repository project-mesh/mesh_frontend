a-button>\n      </a-form-item>\n    </a-form>\n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'Step2',\n  data () {\n    return {\n      labelCol: { lg: { span: 5 }, sm: { span: 5 } },\n      wrapperCol: { lg: { span: 19 }, sm: { span: 19 } },\n      form: this.$form.createForm(this),\n      loading: false,\n      timer: 0\n    }\n  },\n  methods: {\n    nextStep () {\n      const that = this\n      const { form: { validateFields } } = this\n      that.loading = true\n      validateFields((err, values) => {\n        if (!err) {\n          console.log('表单 values', values)\n          that.timer = setTimeout(function () {\n            that.loading = false\n            that.$emit('nextStep')\n          }, 1500)\n        } else {\n          that.loading = false\n        }\n      })\n    },\n    prevStep () {\n      this.$emit('prevStep')\n    }\n  },\n  beforeDestroy () {\n    clearTimeout(this.timer)\n  }\n}\n</script>\n\n<style lang=\"less\" scoped>\n  .stepFormText {\n    margin-bottom: 24px;\n\n    .ant-form-item-label,\n    .ant-form-item-control {\n      line-height: 22px;\n    }\n  }\n\n</style>\n"],"sourceRoot":"src/views/form/stepForm"}]}