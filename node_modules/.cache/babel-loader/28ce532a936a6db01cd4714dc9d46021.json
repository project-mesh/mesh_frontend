rototype, {\n    $enum: {\n      get () {\n        // const _this = this;\n        return (val) => {\n          let result = PERMISSION_ENUM\n          val && val.split('.').forEach(v => {\n            result = result && result[v] || null\n          })\n          return result\n        }\n      }\n    }\n  })\n}\n\nexport default plugin\n"]}]}